[repositories]
root = .
prelude = prelude
toolchains = toolchains
none = none

[repository_aliases]
config = prelude
fbcode = none
fbsource = none
buck = none

[project]
ignore = third-party/.cargo, third-party/vendor, target

[parser]
target_platform_detector_spec = target:root//...->prelude//platforms:default

[rust]
remap_src_paths = yes
rustc_check_flags = --cap-lints=warn
